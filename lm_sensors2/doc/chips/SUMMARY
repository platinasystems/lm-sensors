	adm1021		2	-	-	-	yes	no
	lm84		2	-	-	-	yes	no
	gl523sm		2	-	-	-	yes	no
	max1617		2	-	-	-	yes	no
	max1617a	2	-	-	-	yes	no
	thmc10		2	-	-	-	yes	no
	xeon		1	-	-	-	yes	no
	(xeon - no detection - requires force parameter)

adm9240
	adm9240		1	6	2	1 dac	yes	no
	ds1780		1	6	2	1 dac	yes	no
	lm81		1	6	2	1 dac	yes	no

